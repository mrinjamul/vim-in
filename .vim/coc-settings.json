{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        }
    },
    "efm": {
        "command": "efm-langserver",
        "args": [],
        "args": ["-c", "$HOME/.config/efm-langserver/config.yaml"],
        "filetypes": ["vim", "eruby", "markdown"]
    },
    "coc.preferences.formatOnSaveFiletypes": ["html","css","js", "Markdown"]
}
